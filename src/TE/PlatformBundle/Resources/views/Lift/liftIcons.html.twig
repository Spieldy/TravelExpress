<span class="LiftIcons-span">
  {{lift.lift.dateLift|date('d F Y H:i')}}
</span>
<span class="LiftIcons-span">
  {{lift.seats}} <i class="fa fa-user"></i>
</span>
<span class="LiftIcons-span">
  {{lift.lift.price}}<i class="fa fa-usd"></i>
</span>
{% if is_granted("ROLE_ADMIN") %}
    <span class="LiftIcons-span">
        <a href="{{path('te_admin_removeLift', {'id':lift.lift.id}) }}">
            <i class="fa fa-trash-o"></i>
        </a>
    </span>
{% endif %}